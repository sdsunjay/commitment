<%= title 'Sign Up' %>

<%= render 'partials/alerts' %>

<section>
    <div class='container'>
        <div class='row'>
            <div class='col-lg-offset-2 col-md-offset-1 col-lg-8 col-md-10 col-sm-12'>
                <br/>
                <div class='textcenter'>
                    <h4>Already have an account? Click
                        <%= link_to 'HERE', new_user_session_path %>
                    </h4>
                </div><!-- end .textcenter -->
            </div><!-- end .col-lg-offset-3 .col-md-offset-2 .col-lg-6 .col-md-8 .col-sm-12 -->
            <div class='col-md-offset-1 col-md-10 col-sm-12'>
                <div class='page-content'>
                    <%= render 'form' %>
                </div> <!-- end .page-content --> 
            </div><!-- end .col-lg-offset-3 .col-md-offset-2 .col-lg-6 .col-md-8 .col-sm-12 -->
        </div><!-- end .row --> 
        <div class="row">
            <div class='col-lg-offset-2 col-md-offset-1 col-lg-8 col-md-10 col-sm-12'>
                <%= render 'devise/shared/links' %>
            </div><!-- end .col-lg-offset-2 .col-md-offset-1 .col-lg-8 .col-md-10 .col-sm-12 -->
        </div><!-- end .row --> 
    </div><!-- end .container --> 
</section>
